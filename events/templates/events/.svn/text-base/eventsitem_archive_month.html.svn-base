{% load markup cms_tags %}

{% block events %}
{% placeholder "events" inherit %}
{% endblock %}

{% block title_tag %}
	Events from {{ month|date:"F Y" }}
{% endblock %}

{% block main_content %}
<h1>Events from {{ month|date:"F Y" }}</h1>
	{% for item in item_list %}
		<h2><a href="{{ item.get_absolute_url }}">{{ item.title }}</a></h2>
		<h3>{{ item.date|date:"F j, Y" }}</h3>
		{{ item.body|textile }}
	{% endfor %}
{% endblock %}
