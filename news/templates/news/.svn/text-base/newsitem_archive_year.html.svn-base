{% load markup news_tags cms_tags %}

{% block news %}
{% placeholder "news" inherit %}
{% endblock %}

{% block title_tag %}
	News from {{ year }}
{% endblock %}

{% block main_content %}
<h1>News from {{ year }}</h1>

	<ul>
		{% for item in item_list %}
			<li><strong>{{ item.pub_date|date:"M j" }} &ndash;</strong> <a href="{{ item.get_absolute_url }}">{{ item.title }}</a></li>
		{% endfor %}
	</ul>

{% endblock %}
